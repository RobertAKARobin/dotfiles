[user]
	name = RobertAKARobin
	email = robertgfthomas@gmail.com
[core]
	excludesfile = ~/.gitignore_global
	filemode = false
	editor = vim
	ignorecase = false
[credentials]
	username = robertakarobin
[credential]
	helper = osxkeychain
[push]
	default = simple
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
[merge]
	tool = diffmerge
[mergetool "diffmerge"]
	cmd = "diffmerge --merge --result=\"$MERGED\" \"$LOCAL\" \"$(if test -f \"$BASE\"; then echo \"$BASE\"; else echo \"$LOCAL\"; fi)\" \"$REMOTE\""
	trustExitCode = true
